{
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 140,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"semi": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"htmlWhitespaceSensitivity": "css",
	"parser": "jsonc",
	"endOfLine": "lf"
}
