{
  "name": "NodeSpeed",
  "version": "1.0.0",
  "description": "CLI tool designed to test and improve typing speed. It allows users to take typing speed tests, supports different difficulty levels and languages, and logs results for further analysis.",
  "keywords": [
    "type",
    "speed type",
    "nodejs cli",
    "axios"
  ],
  "type": "module",
  "main": "./main.js",
  "author": {
    "name": "fxhxyz4",
    "email": "fxhsec@proton.me",
    "url": "https://fxhxyz.vercel.app"
  },
  "homepage": "https://github.com/fxhxyz4/nodespeed/",
  "bugs": "https://github.com/fxhxyz4/nodespeed/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/nodespeed.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhsec@proton.me"
    }
  ],
  "license": "NOT MIT",
  "scripts": {
    "dev": "nodemon .",
    "node": "node .",
    "auth-dev": "nodemon ./auth/auth.js",
    "auth-prod": "node ./auth/auth.js",
    "test-h": "node main.js -h",
    "test-a": "node main.js -a",
    "test-v": "node main.js -v",
    "test-contact": "node main.js --contact",
    "test-helpCmd": "node main.js --helpCmd=o",
    "test-c": "node main.js -c=3",
    "test-l": "node main.js -l=ru",
    "test-m": "node main.js -m=timed -t=4000",
    "test-t": "node main.js -t=100",
    "test-s": "node main.js -s=../../tests/file.txt",
    "test-s2": "node main.js -s=https://raw.githubusercontent.com/fxhxyz4/NodeSpeed/refs/heads/main/tests/file.txt",
    "test-stats": "node main.js --stats",
    "test-o": "node main.js -o",
    "test-all-normal": "node main.js -c=1 -l=en -m=normal",
    "test-all-normal-s": "node main.js -c=1 -l=en -m=normal -s=../../tests/file.txt",
    "test-all-timed": "node main.js -c=1 -l=en -m=timed -t=8000",
    "test-all-timed-s": "node main.js -c=1 -l=en -m=timed -t=8000 -s=../tests/file.txt",
    "test": "echo test",
    "prettier": "prettier --write --parser babel ."
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inquirer": "^12.5.0",
    "moment": "^2.30.1",
    "open": "^10.1.0",
    "simple-oauth2": "^5.1.0"
  },
  "engines": {
    "node": "20.x"
  }
}
